{
  "name": "@sylphlab/zen-router-react",
  "version": "0.0.1",
  "description": "React integration for @sylphlab/zen-router",
  "author": "Sylph AI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sylphlab/zen.git",
    "directory": "packages/zen-router-react"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
