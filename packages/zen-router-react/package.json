  "devDependencies": {
    "@testing-library/react": "latest",
    "@types/node": "^22.14.1",
    "@types/react": "^18.3.3",
    "jsdom": "^26.1.0",
    "react": "^18.3.1",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1"
