{
  "name": "@sylphlab/zen-router-preact",
  "version": "0.0.2",
  "description": "Preact integration for @sylphlab/zen-router",
  "author": "Sylph Lab <hi@sylphlab.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sylphlab/zen.git",
    "directory": "packages/zen-router-preact"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "zen",
    "router",
    "preact",
    "hooks",
    "state management"
  ],
  "homepage": "https://github.com/sylphlab/zen/tree/main/packages/zen-router-preact#readme",
  "bugs": {
    "url": "https://github.com/sylphlab/zen/issues"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "clean": "rm -rf dist .turbo node_modules"
  },
  "dependencies": {
    "@sylphlab/zen-core": "workspace:*",
    "@sylphlab/zen-router": "workspace:*"
  },
  "peerDependencies": {
    "preact": "^10.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "preact": "^10.22.1",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5",
    "@preact/preset-vite": "^2.8.2",
    "@testing-library/preact": "^3.2.4",
    "@vitest/coverage-v8": "^1.6.1",
    "jsdom": "^24.1.1",
    "vitest": "^1.6.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}